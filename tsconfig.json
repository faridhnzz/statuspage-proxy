{
	"compilerOptions": {
		"target": "esnext",
		"module": "esnext",
		"strict": true,
		"baseUrl": ".",
		"lib": ["esnext", "WebWorker"],
		"moduleResolution": "node",
		"allowJs": true,
		"noEmit": true,
		"esModuleInterop": true,
		"types": ["@cloudflare/workers-types"]
	},
	"include": ["src/**/*.ts"],
	"exclude": ["node_modules/", "dist/"]
}
